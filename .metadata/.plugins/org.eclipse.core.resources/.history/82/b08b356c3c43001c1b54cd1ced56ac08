package entities;

public class Account {
	
	private int name;
	private String holder;
	private double balance;
	public Account(int name, String holder) {
		
		this.name = name;
		this.holder = holder;
	}
	public Account(int name, String holder, double balance) {
		
		this.name = name;
		this.holder = holder;
		this.balance = balance;
	}
	
	

}
